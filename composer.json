  "authors": [
    {
      "name": "Elliot J. Reed",
      "email": "git-contact@elliotjreed.com",
      "homepage": "https://www.elliotjreed.com",
      "role": "Developer"
    }
  ],
  "type": "project",
  "description": "",
  "autoload": {
    "psr-4" : {
      "ElliotJReed\\DatabaseAnonymiser\\" : "src/ElliotJReed/DatabaseAnonymiser/"
    }
  },
  "autoload-dev": {
    "psr-4" : {
      "Tests\\ElliotJReed\\DatabaseAnonymiser\\" : "tests/ElliotJReed/DatabaseAnonymiser/"
    }
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "phpunit/phpunit": "^7",
    "squizlabs/php_codesniffer": "^3",
    "php-coveralls/php-coveralls": "^2"
  },
  "scripts": {
    "test": [
      "@phpunit",
      "@phpcs"
    ],
    "phpunit": "php vendor/bin/phpunit -c phpunit.xml --coverage-text --testdox-text ./testdox/testdox.txt; cat ./testdox/testdox.txt",
    "phpcs": "php vendor/bin/phpcs --standard=ruleset.xml"
  }
}
